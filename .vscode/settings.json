{
    "git.autofetch": true,
    "gitdoc.enabled": true,
    "gitdoc.autoCommitDelay": 60000,
    "gitdoc.autoPushDelay": 0,
    "gitdoc.commitMessageFormat": "ccc, LLL d, y, h:mm a '+00:00'",
    "flake8.args": [
        "--ignore=E501,F541,F403,E302,F405,E262"
    ],
    "files.insertFinalNewline": true,
    "pylint.args": [
        "--disable=W1309,W0401"
    ],
    "mypy-type-checker.args": [
        "assignment"
    ]
}
